<?xml version="1.0" encoding="UTF-8"?>
<profiles>
  <profile preselected="false">
    <name>{{ defaultUserName }}</name>
    <image resourceType="BUNDLE">users/default.png</image>
    <configuration>
      <rotationSpeed>1</rotationSpeed>
      <defaultGridRow>3</defaultGridRow>
      <defaultGridColumn>3</defaultGridColumn>
      <sound>true</sound>
      <image>true</image>
      <text>true</text>
      <selectionSensor>
        <type>keyboard</type>
        <keyCode>32</keyCode>
        <keyChar />
      </selectionSensor>
    </configuration>
    <communication>
      <name>{{ communicationName }}</name>
      <enabled>true</enabled>
      <image />
      <sound />
      <rows>3</rows>
      <columns>3</columns>
      <categories>
        <category name="{{ categoryYes }}">
          <rows>3</rows>
          <columns>3</columns>
          <image resourceType="BUNDLE">images/yes.png</image>
          <sound resourceType="BUNDLE">sounds/yes.mp3</sound>
          <hasSound>true</hasSound>
          <hasImage>true</hasImage>
          <hasText>true</hasText>
        </category>
        <category name="{{ categoryNo }}">
          <rows>3</rows>
          <columns>3</columns>
          <image resourceType="BUNDLE">images/no.png</image>
          <sound resourceType="BUNDLE">sounds/no.mp3</sound>
          <hasSound>true</hasSound>
          <hasImage>true</hasImage>
          <hasText>true</hasText>
        </category>
        <category name="{{ categoryFood }}">
          <rows>3</rows>
          <columns>3</columns>
          <image resourceType="BUNDLE">images/food/burger.png</image>
          <sound resourceType="BUNDLE">sounds/food/food.mp3</sound>
          <hasSound>false</hasSound>
          <hasImage>false</hasImage>
          <hasText>false</hasText>
          <categories>
            <category name="{{ categoryPasta }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/pasta.png</image>
              <sound resourceType="BUNDLE">sounds/food/pasta.mp3</sound>
              <hasSound>false</hasSound>
              <hasImage>false</hasImage>
              <hasText>false</hasText>
            </category>
            <category name="{{ categoryFrenchFries }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/frenchFries.png</image>
              <sound resourceType="BUNDLE">sounds/food/frenchFries.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryBurger }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/burger.png</image>
              <sound resourceType="BUNDLE">sounds/food/burger.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categorySteak }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/steak.png</image>
              <sound resourceType="BUNDLE">sounds/food/steak.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryChicken }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/chicken.png</image>
              <sound resourceType="BUNDLE">sounds/food/chicken.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categorySoup }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/soup.png</image>
              <sound resourceType="BUNDLE">sounds/food/soup.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryBread }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/bread.png</image>
              <sound resourceType="BUNDLE">sounds/food/bread.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryFrenchToast }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/frenchToast.png</image>
              <sound resourceType="BUNDLE">sounds/food/frenchToast.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryYogurt }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/yogurt.png</image>
              <sound resourceType="BUNDLE">sounds/food/yogurt.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryCucumber }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/cucumber.png</image>
              <sound resourceType="BUNDLE">sounds/food/cucumber.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryTomato }}">
              <rows>3</rows>
              <columns>3</columns>
              <image resourceType="BUNDLE">images/food/tomato.png</image>
              <sound resourceType="BUNDLE">sounds/food/tomato.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
          </categories>
        </category>
      </categories>
    </communication>
    <entertainment>
      <name>{{ entertainmentName }}</name>
      <enabled>true</enabled>
      <image />
      <sound />
      <music>
        <name>{{ musicName }}</name>
        <enabled>true</enabled>
        <image />
        <sound />
        <path />
        <playlistSize>10</playlistSize>
      </music>
      <video>
        <name>{{ videoName }}</name>
        <enabled>true</enabled>
        <image />
        <sound />
        <path />
        <playlistSize>10</playlistSize>
      </video>
    </entertainment>
    <games>
      <name>{{ gamesName }}</name>
      <image />
      <sound />
      <enabled>true</enabled>
      <stimulusReactionGames>
        <name>{{ gameTypeStimulusReaction }}</name>
        <image />
        <sound />
        <winSound />
        <errorSound />
        <enabled>true</enabled>
        <games>
          <game>
            <name>Game 1</name>
            <enabled>true</enabled>
            <image />
            <difficulty>4</difficulty>
            <winSound resourceType="BUNDLE">sounds/games/car.mp3</winSound>
            <errorSound />
            <gameImages>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/stimulus/car/car_1.png</path>
                <order>1</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/stimulus/car/car_2.png</path>
                <order>2</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/stimulus/car/car_3.png</path>
                <order>3</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/stimulus/car/car_4.png</path>
                <order>4</order>
                <enabled>true</enabled>
              </image>
            </gameImages>
          </game>
        </games>
      </stimulusReactionGames>
      <sequenceGames>
        <name>{{ gameTypeTimeSequence }}</name>
        <image />
        <sound />
        <winSound />
        <errorSound />
        <enabled>true</enabled>
        <games>
          <game>
            <name>Game 1</name>
            <enabled>true</enabled>
            <image />
            <difficulty>4</difficulty>
            <winSound />
            <errorSound />
            <gameImages>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/sequence/sea_swimming/sea_swimming_1.png</path>
                <order>1</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/sequence/sea_swimming/sea_swimming_2.png</path>
                <order>2</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/sequence/sea_swimming/sea_swimming_3.png</path>
                <order>3</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/sequence/sea_swimming/sea_swimming_4.png</path>
                <order>4</order>
                <enabled>true</enabled>
              </image>
            </gameImages>
          </game>
        </games>
      </sequenceGames>
      <similarityGames>
        <name>{{ gameTypeFindTheSimilar }}</name>
        <image />
        <sound />
        <winSound resourceType="BUNDLE">sounds/games/winSound.mp3</winSound>
        <errorSound resourceType="BUNDLE">sounds/games/errorSound.mp3</errorSound>
        <enabled>true</enabled>
        <games>
          <game>
            <name>Game 1</name>
            <enabled>true</enabled>
            <image />
            <difficulty>3</difficulty>
            <winSound />
            <errorSound />
            <gameImages>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/similarity/circle/large_blue.png</path>
                <order>1</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/similarity/circle/large_red.png</path>
                <order>2</order>
                <enabled>true</enabled>
              </image>
              <image>
                <name />
                <path resourceType="BUNDLE">images/games/similarity/circle/large_yellow.png</path>
                <order>3</order>
                <enabled>true</enabled>
              </image>
            </gameImages>
          </game>
        </games>
      </similarityGames>
    </games>
  </profile>
</profiles>
