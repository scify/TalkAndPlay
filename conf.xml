<?xml version="1.0" encoding="UTF-8"?>
<profiles>
  <profile preselected="false">
    <name>{{ defaultUserName }}</name>
    <image resourceType="BUNDLE">users/default.png</image>
    <configuration>
      <rotationSpeed>1</rotationSpeed>
      <defaultGridRow>3</defaultGridRow>
      <defaultGridColumn>3</defaultGridColumn>
      <sound>true</sound>
      <image>true</image>
      <text>true</text>
      <selectionSensor>
        <type>keyboard</type>
        <keyCode>32</keyCode>
        <keyChar />
      </selectionSensor>
    </configuration>
    <communication>
      <name>{{ communicationName }}</name>
      <enabled>true</enabled>
      <image />
      <sound />
      <rows>3</rows>
      <columns>3</columns>
      <categories>
        <category name="{{ categoryYes }}">
          <rows>3</rows>
          <columns>3</columns>
          <image>images/yes.png</image>
          <sound>sounds/yes.mp3</sound>
          <hasSound>true</hasSound>
          <hasImage>true</hasImage>
          <hasText>true</hasText>
        </category>
        <category name="{{ categoryNo }}">
          <rows>3</rows>
          <columns>3</columns>
          <image>images/no.png</image>
          <sound>sounds/no.mp3</sound>
          <hasSound>true</hasSound>
          <hasImage>true</hasImage>
          <hasText>true</hasText>
        </category>
        <category name="{{ categoryFood }}">
          <rows>3</rows>
          <columns>3</columns>
          <image>images/food/burger.png</image>
          <sound>sounds/food/food.mp3</sound>
          <hasSound>false</hasSound>
          <hasImage>false</hasImage>
          <hasText>false</hasText>
          <categories>
            <category name="{{ categoryPasta }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/pasta.png</image>
              <sound>sounds/food/pasta.mp3</sound>
              <hasSound>false</hasSound>
              <hasImage>false</hasImage>
              <hasText>false</hasText>
            </category>
            <category name="{{ categoryFrenchFries }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/frenchFries.png</image>
              <sound>sounds/food/frenchFries.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryBurger }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/burger.png</image>
              <sound>sounds/food/burger.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categorySteak }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/steak.png</image>
              <sound>sounds/food/steak.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryChicken }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/chicken.png</image>
              <sound>sounds/food/chicken.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categorySoup }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/soup.png</image>
              <sound>sounds/food/soup.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryBread }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/bread.png</image>
              <sound>sounds/food/bread.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryFrenchToast }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/frenchToast.png</image>
              <sound>sounds/food/frenchToast.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryYogurt }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/yogurt.png</image>
              <sound>sounds/food/yogurt.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryCucumber }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/cucumber.png</image>
              <sound>sounds/food/cucumber.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
            <category name="{{ categoryTomato }}">
              <rows>3</rows>
              <columns>3</columns>
              <image>images/food/tomato.png</image>
              <sound>sounds/food/tomato.mp3</sound>
              <hasSound>true</hasSound>
              <hasImage>true</hasImage>
              <hasText>true</hasText>
            </category>
          </categories>
        </category>
      </categories>
    </communication>
    <entertainment>
      <name>{{ entertainmentName }}</name>
      <enabled>true</enabled>
      <image />
      <sound />
      <music>
        <name>{{ musicName }}</name>
        <enabled>true</enabled>
        <image />
        <sound />
        <path />
        <playlistSize>10</playlistSize>
      </music>
      <video>
        <name>{{ videoName }}</name>
        <enabled>true</enabled>
        <image />
        <sound />
        <path />
        <playlistSize>10</playlistSize>
      </video>
    </entertainment>
    <games>
      <name>{{ gamesName }}</name>
      <image />
      <sound />
      <enabled>true</enabled>
    </games>
  </profile>
</profiles>
