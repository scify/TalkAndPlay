                    <game name="Game 2" enabled="true">
                        <image/>
                        <difficulty>4</difficulty>
                        <winSound/>
                        <errorSound/>
                        <gameImages>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/flower_growing/flower_growing_1.png
                                </path>
                                <order>1</order>
                                <enabled>true</enabled>
                            </image>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/flower_growing/flower_growing_2.png
                                </path>
                                <order>2</order>
                                <enabled>true</enabled>
                            </image>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/flower_growing/flower_growing_3.png
                                </path>
                                <order>3</order>
                                <enabled>true</enabled>
                            </image>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/flower_growing/flower_growing_4.png
                                </path>
                                <order>4</order>
                                <enabled>true</enabled>
                            </image>
                        </gameImages>
                    </game>
                    <game name="Game 3" enabled="true">
                        <image/>
                        <difficulty>4</difficulty>
                        <winSound/>
                        <errorSound/>
                        <gameImages>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/dressing/dressing_1.png</path>
                                <order>1</order>
                                <enabled>true</enabled>
                            </image>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/dressing/dressing_2.png</path>
                                <order>2</order>
                                <enabled>true</enabled>
                            </image>
                            <image>
                                <name/>
                                <path resourceType="BUNDLE">images/games/sequence/dressing/dressing_3.png</path>
                                <order>3</order>
                                <enabled>true</enabled>
                            </image>
